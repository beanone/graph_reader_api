[run]
source = src/graph_reader_api
omit =
    */tests/*
    */__init__.py
    */migrations/*
data_file = .coverage

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
show_missing = True

[html]
directory = coverage_html

[paths]
source =
    src/graph_reader_api
    */site-packages/graph_reader_api
